{
  "name": "chaos",
  "displayName": "chaos",
  "description": "Chaos is a vscode plugin to add markdown grammer `` for your english words in markdown.",
  "version": "0.0.8",
  "publisher": "xu",
  "engines": {
    "vscode": "^1.18.0"
  },
  "icon": "images/logo.png",
  "categories": [
    "Other"
  ],
  "keywords": [
    "pangu",
    "chaos",
    "markdown",
    "tag"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MyHerux/vscode-chaos.git"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.add_chaos",
        "title": "Chaos: Add chaos for selection"
      },
      {
        "command": "extension.add_chaos_en",
        "title": "Chaos: Add chaos_en for selection"
      }
    ],
    "keybindings": [
      {
        "command": "extension.add_chaos_en",
        "key": "alt+c"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^5.2.0",
    "typescript": "^2.0.3",
    "vscode": "^1.1.18"
  },
  "dependencies": {
    "lodash": "^4.17.10"
  }
}
